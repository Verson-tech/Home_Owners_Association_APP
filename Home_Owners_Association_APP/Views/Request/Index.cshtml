@model IEnumerable<Request>

<h2>Requests</h2>

<div style="display: block">
    <a href="/Request/InsertRequest/">Create a New Request</a>
</div>
<table class="table">
    <tr>
        <th>RequestID</th>
        <th>RequestDESC</th>
        <th>RequestSTATUS</th>
    </tr>

    @foreach (var i in @Model)
    {
        <tr>
            @*<td>@i.RequestID</td>*@
            <td><a href=/Request/ViewRequest/@i.RequestID>@i.RequestID</a></td>
            @*<td>@i.RequestDESC</td>*@
            <td><a href=/Request/ViewRequest/@i.RequestID>@i.RequestDESC</a></td>
            <td>@i.RequestSTATUS</td>
        </tr>
    }

</table>